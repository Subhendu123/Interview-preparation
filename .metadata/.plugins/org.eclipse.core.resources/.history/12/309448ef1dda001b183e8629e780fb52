@Repository
public class ConnectionDAO extends SimpleJpaRepository<TestTable, Integer>{

	public ConnectionDAO(JpaEntityInformation<TestTable, Integer> entityInformation, EntityManager entityManager) {
		super(entityInformation, entityManager);
		// TODO Auto-generated constructor stub
	}

	

}
